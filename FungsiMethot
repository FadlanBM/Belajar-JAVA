package com.company;

public class FungsiMethot {
    public static void main (String[] args){
        int x,y;
        x=5;
        y=hitung(x);
        System.out.printf("x=%d ,y=%d\n",x,y);

        x=10;
        y=hitung(x);
        System.out.printf("x=%d ,y=%d\n",x,y);

        x=20;
        y=hitung(x);
        System.out.printf("x=%d ,y=%d\n",x,y);
    }
    private static int hitung(int input){
        int hasil;

        hasil =(input+2)*input;

        return hasil;
    }
}
